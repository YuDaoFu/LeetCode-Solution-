class Solution:
    def removeNthFromEnd(self, head: Optional[ListNode], n: int) -> Optional[ListNode]:
        # get the number of elements in the linkedlist
        cur = head
        count = 0
        while cur:
            cur = cur.next
            count +=1

        # create dummy variable
        dummy = ListNode(next = head)
        cur = dummy
        i = 0
        # if n >= count, return empty
        if n>=count:
            return head.next
        else:
            # else move the pinter to the element in front nth and point it to the next element
            while i<count-n:
                cur = cur.next
                i +=1
            cur.next = cur.next.next

        return dummy.next
